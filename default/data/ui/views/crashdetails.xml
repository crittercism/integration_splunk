<form script="link_switcher.js, autodiscover.js">
  <label>Crash Details</label>
  <fieldset autoRun="true" submitButton="false">
    <input type="dropdown" token="hash" searchWhenChanged="true">
      <label>hash</label>
      <populatingSearch fieldForValue="hash" fieldForLabel="reason">
        <![CDATA[index=crittercism sourcetype=crittercism MessageType=CrashSummary earliest=-7d | dedup reason,name | top hash]]>
      </populatingSearch>
    </input>
    <input type="dropdown" token="app_version" searchWhenChanged="true">
      <label>Version</label>
      <populatingSearch fieldForValue="appVersion" fieldForLabel="appVersion">
        <![CDATA[index=crittercism sourcetype=crittercism hash=$hash$ MessageType=CrashDetailBreadcrumbs earliest=-7d| top appVersion]]>
      </populatingSearch>
      <selectFirstChoice>true</selectFirstChoice>
    </input>
    <input type="dropdown" token="os" searchWhenChanged="true">
      <label>Operating System</label>
      <populatingSearch fieldForValue="os" fieldForLabel="os">
        <![CDATA[index=crittercism sourcetype=crittercism hash=$hash$ MessageType=CrashDetailBreadcrumbs earliest=-7d| top os]]>
      </populatingSearch>
      <selectFirstChoice>true</selectFirstChoice>
    </input>
    <input type="dropdown" token="device">
      <label>Device</label>
      <populatingSearch fieldForValue="device" fieldForLabel="device">
        <![CDATA[index=crittercism sourcetype=crittercism hash=$hash$ MessageType=CrashDetailBreadcrumbs earliest=-7d| top device]]>
      </populatingSearch>
      <selectFirstChoice>true</selectFirstChoice>
    </input>
  </fieldset>
  <row>
    <panel>
      <html>
      <!-- Use the web frameworks auto-discovery to create a new link switcher view -->
      <div id="switcher2" class="splunk-view" data-require="app/simple_xml_examples/components/linkswitcher/linkswitcher" data-options="{                     &quot;items&quot;: [&quot;link4&quot;,&quot;link5&quot;]                  }"/>
    </html>
      <table id="link4">
        <title>Detailed Stacktrace</title>
        <searchString>index=crittercism sourcetype=crittercism MessageType="CrashDetailStacktrace"  hash=$hash$ | head 1 | rex "(?s)hash=(?&lt;hash2&gt;[a-z0-9]+)\s(?&lt;data&gt;.*)" | eval data2=replace(data,"(u\')|\'|\[|\]"," ") | eval data2=replace(data2,"(\\\t)"," ")| makemv delim="," data2 | chart list(data2) as DETAIL</searchString>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Breadcrumbs</title>
        <searchString>
          <![CDATA[index=crittercism sourcetype=crittercism MessageType=CrashDetailBreadcrumbs hash=$hash$ | head 1 | eval trace = replace(trace, "'payload': ", " ") | eval trace = split(trace, "|") | table os, appVersion, trace]]>
        </searchString>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Crashes By Carrier</title>
        <searchString>* index=crittercism sourcetype=crittercism MessageType=CrashDiagsDiscrete "hash=$hash$" | head 1| `discrete_vars`| search VAR=carrier* |rex field=VAR "(?&lt;GROUP&gt;.*:)?(?&lt;VARX&gt;.*)"| eval Carrier=VARX| eval Carrier=replace(Carrier,"\%26","&amp;") |  chart list(VAL) AS Percentage by Carrier</searchString>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <table>
        <title>Crashes by System Version</title>
        <searchString>* index=crittercism sourcetype=crittercism MessageType=CrashDiagsDiscrete "hash=$hash$"  | head 1 | rex "hash=[0-9a-f]*(?&lt;DATA&gt;.*)$"  | rex field=DATA max_match=50 "\"(?&lt;var&gt;.*?)\"\=\"(?&lt;val&gt;.*?)\""  | eval fields=mvzip(var,val) | mvexpand fields  | rex field=fields "(?&lt;VAR&gt;.*),(?&lt;VAL&gt;\d+)" | rex field=VAR "(?&lt;GROUP&gt;.*:)?(?&lt;VARX&gt;.*)"  | search GROUP = "system_version:" | rename VARX as Version, VAL as Crashes | table  Version, Crashes</searchString>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Top Breadcrumb URLs</title>
        <search>
          <query>index=crittercism sourcetype=crittercism MessageType=CrashDetailBreadcrumbs hash=$hash$ | head 100 | search 'url' | rex "'url': '(?&lt;url&gt;.*?)'," | dedup url | table url</query>
        </search>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
        <option name="list.drilldown">full</option>
        <option name="list.wrap">1</option>
        <option name="maxLines">5</option>
        <option name="raw.drilldown">full</option>
        <option name="table.drilldown">all</option>
        <option name="table.wrap">1</option>
        <option name="type">list</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table id="master">
        <title>Discrete Groups Available for Report (Click for detail)</title>
        <searchString>* index=crittercism sourcetype=crittercism MessageType=CrashDiagsDiscrete "hash=$hash$" | head 1| `discrete_vars`| rex field=VAR "(?&lt;GROUP&gt;.*:)?(?&lt;VARX&gt;.*)"| eval GROUP=replace(GROUP,"\:","") | dedup GROUP | table GROUP</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="count">10</option>
        <drilldown>
          <!-- Use set to specify the new token to be created.
                     Use any token from the page or from the click event to produce the value needed. -->
          <set token="GROUP">$row.GROUP$</set>
          <!-- If we also set the form.sourcetype the input will get updated too -->
          <!-- set token="form.sourcetype">$row.sourcetype$</set> -->
          <link>
            <![CDATA[
/app/crittercism/data_drilldown?form.mygroup=$click.value2$&hash=$hash$
  ]]>
          </link>
        </drilldown>
      </table>
    </panel>
    <panel>
      <table>
        <title>Users Affected - click username for more detail</title>
        <searchString>index=crittercism sourcetype=crittercism MessageType="CrashDiagsAffectedUser"  hash=$hash$ | dedup username| table username,userhash</searchString>
        <earliestTime>-7d@h</earliestTime>
        <latestTime>now</latestTime>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="count">10</option>
        <drilldown>
          <link>
            <![CDATA[/app/crittercism/user_details?userhash=$row.userhash$
       ]]>
          </link>
        </drilldown>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Disk Stats (Continuous Diags)</title>
        <searchString>index=crittercism sourcetype=crittercism MessageType=CrashDiagsContinuous "hash=$hash$"| head 30| rex "hash=[0-9a-f]*(?&lt;DATA&gt;.*)$" | rex field=DATA max_match=50 "(?&lt;var&gt;.*?)\=\"(?&lt;val&gt;.*?)\"" | eval pair=var."=".val | mvexpand pair  | eval disk_space_free_average=replace(disk_space_free_average,"MB","")| eval disk_space_free_max=replace(disk_space_free_max,"MB","")| eval disk_space_free_min=replace(disk_space_free_min,"MB","") |chart max(disk_space_free_max) AS MaxFree, max(disk_space_free_min) as MinFree, max(disk_space_free_average) as AvgFree  by _time</searchString>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart">line</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">default</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
    <panel>
      <map>
        <searchString>index=crittercism sourcetype=crittercism CrashDiagsGeo "hash=$hash$" | dedup city | geostats latfield=lat longfield=lon list(crashes) AS crashes</searchString>
        <earliest>-30d@d</earliest>
        <latest>now</latest>
        <option name="mapping.data.maxClusters">100</option>
        <option name="mapping.drilldown">all</option>
        <option name="mapping.map.center">(0,0)</option>
        <option name="mapping.map.zoom">2</option>
        <option name="mapping.markerLayer.markerMaxSize">50</option>
        <option name="mapping.markerLayer.markerMinSize">10</option>
        <option name="mapping.markerLayer.markerOpacity">0.8</option>
        <option name="mapping.tileLayer.maxZoom">7</option>
        <option name="mapping.tileLayer.minZoom">0</option>
        <option name="drilldown">all</option>
      </map>
    </panel>
  </row>
</form>